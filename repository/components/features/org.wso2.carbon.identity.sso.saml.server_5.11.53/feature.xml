<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<feature id="org.wso2.carbon.identity.sso.saml.server" label="Identity SAML Single Sign-on Server Feature" provider-name="%providerName" version="5.11.53">
  <description>This feature contains the core bundles required for Back-end SAML2.0 based Single
        Sign-on functionality</description>
  <copyright>%copyright</copyright>
  <license url="%licenseURL">%license</license>
  <plugin id="org.apache.santuario.xmlsec" unpack="false" version="2.3.4"/>
  <plugin id="joda-time" unpack="false" version="2.9.4.wso2v1"/>
  <plugin id="jsr105" unpack="false" version="1.0.1.wso2v1"/>
  <plugin id="org.wso2.carbon.identity.sso.saml.common" unpack="false" version="5.11.53"/>
  <plugin id="org.wso2.carbon.identity.sso.saml" unpack="false" version="5.11.53"/>
  <plugin id="opensaml" unpack="false" version="3.3.1.wso2v12"/>
  <require>
    <import feature="org.wso2.carbon.identity.application.mgt.server" match="greaterOrEqual" version="7.7.269"/>
    <import feature="org.wso2.carbon.identity.application.authentication.framework.server" match="greaterOrEqual" version="7.7.269"/>
    <import feature="org.wso2.carbon.identity.saml.common.util" match="equivalent" version="1.4.0"/>
    <import feature="org.wso2.carbon.identity.core.server" match="greaterOrEqual" version="7.7.269"/>
    <import feature="org.wso2.carbon.idp.mgt.server" match="greaterOrEqual" version="7.7.269"/>
    <import feature="org.wso2.carbon.core" match="compatible" version="4.10.37"/>
  </require>
</feature>
